<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sandbox.repository.SampleLikeMapper">

    <select id="findAll" resultType="String">
        select *
        from samplelike;
    </select>

    <select id="findByPk" resultType="String">
        select *
        from samplelike
        where strcol = #{pk}
    </select>

</mapper>
